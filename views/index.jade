extends layout

block content 
	include  SideNav 
	div(class='container')  
		div(class='row')
			div(class='col s12') 
				h1=title
		div(class='row')
			form(class='col s12')
				div(class='row')
					div(class='input-field col s6')
						label(for='cedula') cedula
						input#cedula.validate(type='text')
				div(class='row')
					div(class='input-field col s12')
						label(for='nombres') Nombres y Apellidos
						input#nombres.validate(type='text')
				div(class='row')
					div(class='input-field col s3')
						label(for='nivel') Nivel
						input#nivel.validate(type='text')


				
       




    



